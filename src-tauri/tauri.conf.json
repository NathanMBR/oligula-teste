{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Oligula Teste",
    "version": "0.8.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      },
      "notification": {
        "all": true
      }
    },
    "updater": {
      "active": true,
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDg2RDA0QzQxOUFBMkY0OUMKUldTYzlLS2FRVXpRaHBYZnBSUitEandMWGFrUDdjbTlRa1R0UTVab1YvRzJPYXhQNXhsL2lGelMK",
      "endpoints": [
        "https://github.com/NathanMBR/oligula-teste/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      }
    },
    "windows": [
      {
        "title": "Oligula Teste",
        "width": 1280,
        "height": 720,
        "minWidth": 800,
        "minHeight": 300
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.nathanm.oligula-teste",
      "resources": [
        "./icons/*"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
